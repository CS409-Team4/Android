<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jangyoungsoo.iui_android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LayoutsActivity"
            android:label="Layouts"></activity>
        <activity
            android:name=".UserProfileActivity"
            android:label="User profile"></activity>
        <activity
            android:name=".ConferenceAgendaActivity"
            android:label="Conference agenda"></activity>
        <activity
            android:name=".ItemLayoutsActivity"
            android:label="Item layouts"></activity>
        <activity
            android:name=".SelectionActivity"
            android:label="Selection"></activity>
    </application>

</manifest>